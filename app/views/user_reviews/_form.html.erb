<div class="form-group">
  <div class="col-sm-2">
    <%= f.label :star_master_id, class: "control-label" %>
  </div>
  <div class="col-sm-10">
    <% if params[:action] == "show" %>
      <%= star_image_tag(@UserReview.star_master)%>
    <% else %>
      <%= f.radio_button :star_master_id, 1,  class: "radio-inline" %><%= f.label t(".star1") %>
      <%= f.radio_button :star_master_id, 2,  class: "radio-inline" %><%= f.label t(".star2") %>
      <%= f.radio_button :star_master_id, 3, checked: (f.object.new_record? or  @UserReview.star_master_id == 3),class: "radio-inline" %><%= f.label t(".star3") %>
      <%= f.radio_button :star_master_id, 4,  class: "radio-inline" %><%= f.label t(".star4") %>
      <%= f.radio_button :star_master_id, 5,  class: "radio-inline" %><%= f.label t(".star5") %>
    <% end %>
  </div>
</div>
<div class="form-group">
  <div class="col-sm-2">
    <%= f.label :title, class: "control-label" %>
  </div>
  <div class="col-sm-10">
    <%= f.text_field :title, disabled: params[:action] == "show", class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <div class="col-sm-2">
    <%= f.label :detail %>
  </div>
  <div class="col-sm-10">
    <%= f.text_area :detail, disabled: params[:action] == "show", rows: "5", class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.hidden_field :booking_id %>
</div>
